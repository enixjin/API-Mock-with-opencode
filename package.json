{
  "name": "electron-openapi-mock",
  "version": "1.0.0",
  "description": "Electron app for uploading OpenAPI specs and running mock servers",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [
    "electron",
    "openapi",
    "swagger",
    "mock-server"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "js-yaml": "^4.1.0",
    "json-server": "^1.0.0-alpha.23",
    "swagger-ui-dist": "^5.31.0"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.example.electron-openapi-mock",
    "productName": "OpenAPI Mock Server",
    "mac": {
      "target": "default"
    },
    "directories": {
      "output": "dist"
    }
  }
}
